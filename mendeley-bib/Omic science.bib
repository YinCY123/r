file = {:C$\backslash$:/Users/YinCY/Documents/Mendeley Desktop/Cloonan et al/Nature Methods/Cloonan et al. - 2008 - Stem cell transcriptome profiling via massive-scale mRNA sequencing.pdf:pdf},
isbn = {1548-7105 (Electronic)$\backslash$n1548-7091 (Linking)},
issn = {15487091},
journal = {Nature Methods},
number = {7},
pages = {613--619},
