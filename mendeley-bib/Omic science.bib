@article{Olivon2017,
abstract = {Molecular networking is becoming more and more popular into the metabolomic community to organize tandem mass spectrometry (MS 2) data. Even though this approach allows the treatment and comparison of large data sets, several drawbacks related to the MS-Cluster tool routinely used on the Global Natural Product Social Molecular Networking platform (GNPS) limit its potential. MS-Cluster cannot distinguish between chromatography well-resolved isomers as retention times are not taken into account. Annotation with predicted chemical formulas is also not implemented and semiquantification is only based on the number of MS 2 scans. We propose to introduce a data-preprocessing workflow including the preliminary data treatment by MZmine 2 followed by a homemade Python script freely available to the community that clears the major previously mentioned GNPS drawbacks. The efficiency of this workflow is exemplified with the analysis of six fractions of increasing polarities obtained from a sequential supercritical CO 2 extraction of Stillingia lineata leaves. A mong the several mass spectrometry based metabolomics workflows recently developed, 1,2 Molecular Networking (MN) has appeared as a promising approach to dereplicate complex natural product mixtures. 3 This tool is becoming a standard for metabolomics analysis and gathers together a community of nearly 10000 users from 100 countries. MN significantly facilitates the tandem mass spectrometry (MS 2) data classification by organizing spectra sharing homologous fragmentations into a user-friendly visual representation. Assuming that similar molecules share similar fragmentation patterns, structurally related compounds are prone to cluster together within a network. Unlike many other dereplication strategies, MN has the unique advantage to identify known compounds through the integration of authentic standards or comparison with spectral libraries, but also to target unknown analogues more efficiently. Thus, standard MS 2 data act as "structural landmarks", enabling the annotation of related clusters of spectra. Despite valuable outputs, some improvements are still required to generate unambiguous molecular networks. Initially developed for proteomics purposes, the MS-Cluster algorithm implemented on the Global Natural Product Social Molecular Networking platform (GNPS) platform was developed for the identification of redundant spectra from a same peptide and their replacement with a single averaged spectrum. 4 Based on the premise that occurrence of truly isobaric peptides eluting at different retention times (RTs) in liquid chromatography (LC) is rather uncommon in proteomics, MS-Cluster developers did not integrate RT information for restricted clustering over limited RT ranges. However, in metabolomics samples, the separation of isomers is common. By activating the MS-Cluster option, groups of isomers will ineluctably gather together within the same nodes leading to merge MS 2 data unexpectedly. Furthermore, GNPS quantification system simply relies on the number of MS 2 spectra detected for each precursor ion. This method is highly dependent on the data-dependent acquisition (DDA) mode parameters (number of precursor ions per scan, ion intensity threshold, exclusion rules). Thus, GNPS quantification only leads to a rough approximation of relative or absolute abundances. Finally, annotation of molecular networks by molecular formula generated from any commercial or freely available algorithm is not currently implemented. In the meantime, to face the increasing amount of data generated by untargeted LC-MS analyses of complex mixtures, numerous free software suites, such as XCMS, 5 MetaboAna-lyst, 6 OpenMS, 7 MaxQuant, 8 or SMART, 9 have been developed. These programs include sophisticated computational pipelines that typically consist of peak picking, integration, linear or nonlinear alignment, and normalization steps in order to extract meaningful information. Among them, MZmine 2 is one of the most popular and user-friendly software packages for metabolomics. 10 Its motivation is to propose straightforward operations for nonexpert users and a flexible and modular platform for MS data processing.},
author = {Olivon, Florent and Grelier, Gwendal and Roussi, Fanny and Litaudon, Marc and Touboul, David},
doi = {10.1021/acs.analchem.7b01563},
file = {:C$\backslash$:/Users/YinCY/Documents/Mendeley Desktop/Olivon et al/Analytical Chemistry/Olivon et al. - 2017 - MZmine 2 Data-Preprocessing to Enhance Molecular Networking Reliability.pdf:pdf},
issn = {15206882},
journal = {Analytical Chemistry},
number = {15},
pages = {7836--7840},
pmid = {28644610},
title = {{MZmine 2 Data-Preprocessing to Enhance Molecular Networking Reliability}},
volume = {89},
year = {2017}
}
@article{Chen2019a,
abstract = {Accurate prediction of antigen presentation by human leukocyte antigen (HLA) class II molecules would be valuable for vaccine development and cancer immunotherapies. Current computational methods trained on in vitro binding data are limited by insufficient training data and algorithmic constraints. Here we describe MARIA (major histocompatibility complex analysis with recurrent integrated architecture; https://maria.stanford.edu/ ), a multimodal recurrent neural network for predicting the likelihood of antigen presentation from a gene of interest in the context of specific HLA class II alleles. In addition to in vitro binding measurements, MARIA is trained on peptide HLA ligand sequences identified by mass spectrometry, expression levels of antigen genes and protease cleavage signatures. Because it leverages these diverse training data and our improved machine learning framework, MARIA (area under the curve = 0.89-0.92) outperformed existing methods in validation datasets. Across independent cancer neoantigen studies, peptides with high MARIA scores are more likely to elicit strong CD4+ T cell responses. MARIA allows identification of immunogenic epitopes in diverse cancers and autoimmune disease.},
