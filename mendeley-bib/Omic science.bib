TREM-2, IRAK-M, CD206, TGFbR2, TNFAIP3(A20), CCR2, CCR7, TNF-a, IL-10},
author = {Zimmerman, Kurt A. and Bentley, Melissa R. and Lever, Jeremie M. and Li, Zhang and Crossman, David K. and Song, Cheng Jack and Liu, Shanrun and Crowley, Michael R. and George, James F. and Mrug, Michal and Yoder, Bradley K.},
doi = {10.1681/ASN.2018090931},
file = {:C$\backslash$:/Users/YinCY/Documents/Mendeley Desktop/Zimmerman et al/Journal of the American Society of Nephrology/Zimmerman et al. - 2019 - Single-cell RNA sequencing identifies candidate renal resident macrophage gene expression signatures across sp.pdf:pdf},
