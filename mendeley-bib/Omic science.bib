@article{Gonzalez-Granda2018,
abstract = {{\textless}p{\textgreater}Background: The consumption of high amounts of fructose is associated with metabolic diseases. However, the underlying mechanisms are largely unknown. Objective: To determine the effects of high fructose intake on plasma metabolomics. Study design: We enrolled 12 healthy volunteers (six lean and six obese women, age 24–35 years) in a crossover intervention study. All participants carried out three diets: (1) low fructose ({\textless}10 g/day); (2) high fructose (100 g/day) from natural food sources (fruit); and (3) high fructose (100 g/day) from high fructose syrup (HFS). Outcome measures: The primary outcome was changes in plasma metabolites measured by targeted metabolomics. Results: High compared to low fructose diets caused a marked metabolite class separation, especially because of changes in acylcarnitine and lysophosphatidylcholine levels. Both high fructose diets resulted in a decrease in mean acylcarnitine levels in all subjects, and an increase in mean lysophosphatidylcholine and diacyl-phosphatidylcholine levels in obese individuals. Medium chain acylcarnitines were negatively correlated with serum levels of liver enzymes and with the fatty liver index. Discussion: The metabolic shifts induced by high fructose consumption suggest an inhibition of mitochondrial $\beta$-oxidation and an increase in lipid peroxidation. The effects tended to be more pronounced following the HFS than the fruit diet.{\textless}/p{\textgreater}},
author = {Gonzalez-Granda, Anita and Damms-Machado, Antje and Basrai, Maryam and Bischoff, Stephan},
doi = {10.3390/nu10091254},
file = {:C$\backslash$:/Users/YinCY/Documents/Mendeley Desktop/Gonzalez-Granda et al/Nutrients/Gonzalez-Granda et al. - 2018 - Changes in Plasma Acylcarnitine and Lysophosphatidylcholine Levels Following a High-Fructose Diet A Targ.pdf:pdf},
issn = {2072-6643},
journal = {Nutrients},
keywords = {fructose,liver disease,metabolic syndrome,targeted metabolomics},
number = {9},
pages = {1254},
pmid = {30200659},
title = {{Changes in Plasma Acylcarnitine and Lysophosphatidylcholine Levels Following a High-Fructose Diet: A Targeted Metabolomics Study in Healthy Women}},
url = {http://www.mdpi.com/2072-6643/10/9/1254},
volume = {10},
year = {2018}
}
