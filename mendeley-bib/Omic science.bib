@article{Vinaixa2016,
abstract = {At present, mass spectrometry (MS)-based metabolomics has been widely used to obtain new insights into human, plant, and microbial biochemistry; drug and biomarker discovery; nutrition research; and food control. Despite the high research interest, identifying and characterizing the structure of metabo- lites has become amajor drawback for converting rawMS data into biological knowledge. Comprehensive and well-annotated MS-based spectral databases play a key role in serving this purpose via the forma- tion of metabolite annotations. The main characteristics of the mass spectral databases currently used in MS-based metabolomics are reviewed in this study, underlining their advantages and limitations. In addition, the overlap of compounds with MSn (n ≥ 2) spectra fromauthentic chemical standards in most public and commercial databases has been calculated for the first time. Finally, future prospects of mass spectral databases are discussed in terms of the needs posed by novel applications and instrumental advancements.},
author = {Vinaixa, Maria and Schymanski, Emma L and Neumann, Steffen and Navarro, Miriam},
doi = {10.1016/j.trac.2015.09.005},
