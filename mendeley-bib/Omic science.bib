pmid = {28659722},
title = {{Comparison of Alternative Splicing Junction Detection Tools Using RNASeq Data}},
url = {http://www.eurekaselect.com/openurl/content.php?genre=article{\&}issn=1389-2029{\&}volume=18{\&}issue=3{\&}spage=268},
volume = {18},
