@article{Oreilly1997,
abstract = {We previously identified the angiogenesis inhibitor angiostatin. Using a similar strategy, we have identified endostatin, an angiogenesis inhibitor produced by hemangioendothelioma. Endostatin is a 20 kDa C-terminal fragment of collagen XVIII. Endostatin specifically inhibits endothelial proliferation and potently inhibits angiogenesis and tumor growth. By a novel method of sustained release, E. coli–derived endostatin was administered as a nonrefolded suspension. Primary tumors were regressed to dormant microscopic lesions. Immunohistochemistry revealed blocked angiogenesis accompanied by high prolifera tion balanced by apoptosis in tumor cells. There was no toxicity. Together with angiostatin data, these find ings validate a strategy for identifying endogenous angiogenesis inhibitors, suggest a theme of fragments of proteins as angiogenesis inhibitors, and demonstrate dormancy therapy.},
author = {{O 'reilly}, Michael S and Boehm, Thomas and Shing, Yuen and Fukai, Naomi and Vasios, George and Lane, William S and Flynn, Evelyn and Birkhead, James R and Olsen, Bjorn R and Folkman, Judah},
