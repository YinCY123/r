file = {:home/yincy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Allen, Greiner, Wishart - 2014 - Competitive fragmentation modeling of ESI-MSMS spectra for putative metabolite identification.pdf:pdf},
isbn = {1573-3882},
issn = {15733890},
journal = {Metabolomics},
keywords = {MS/MS,Machine learning,Metabolite identification,Tandem mass spectrometry},
number = {1},
pages = {98--110},
title = {{Competitive fragmentation modeling of ESI-MS/MS spectra for putative metabolite identification}},
volume = {11},
year = {2014}
}
