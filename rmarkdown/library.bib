journal = {Nature},
keywords = {Computational biology and bioinformatics,Epigenomics,Gene regulation,Immunology,Systems biology},
number = {August 2019},
pages = {1--7},
publisher = {Springer US},
title = {{Structural cells are key regulators of organ-specific immune responses}},
