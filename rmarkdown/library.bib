isbn = {1097-4172 (Electronic)\r0092-8674 (Linking)},
issn = {00928674},
journal = {Cell},
number = {1},
pages = {100--110},
pmid = {22682761},
title = {{The structure of human argonaute-2 in complex with miR-20a}},
volume = {150},
year = {2012}
}
