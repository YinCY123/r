doi = {10.1186/1471-2164-10-164},
file = {:home/yincy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Booij et al. - 2009 - Functional annotation of the human retinal pigment epithelium transcriptome.pdf:pdf},
isbn = {1471216410164},
issn = {14712164},
journal = {BMC Genomics},
pages = {1--18},
pmid = {19379482},
title = {{Functional annotation of the human retinal pigment epithelium transcriptome}},
volume = {10},
year = {2009}
