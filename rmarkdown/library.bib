file = {:home/yincy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Pein et al. - 2020 - Metastasis-initiating cells induce and exploit a fibroblast niche to fuel malignant colonization of the lungs.pdf:pdf},
issn = {20411723},
journal = {Nature Communications},
number = {1},
pages = {1--18},
pmid = {32198421},
publisher = {Springer US},
title = {{Metastasis-initiating cells induce and exploit a fibroblast niche to fuel malignant colonization of the lungs}},
