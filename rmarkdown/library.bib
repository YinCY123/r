file = {:home/yincy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Crabb et al. - 2002 - Drusen proteome analysis An approach to the etiology of age-related macular degeneration.pdf:pdf},
issn = {00278424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
number = {23},
pages = {14682--14687},
pmid = {12391305},
title = {{Drusen proteome analysis: An approach to the etiology of age-related macular degeneration}},
volume = {99},
