issn = {20505701},
journal = {Microscopy (Oxford, England)},
keywords = {Cryo-EM,biochemistry,high-resolution,macromolecular complexes,sample optimization},
number = {1},
pages = {23--34},
title = {{Sample preparation of biological macromolecular assemblies for the determination of high-resolution structures by cryo-electron microscopy}},
volume = {65},
year = {2016}
}
