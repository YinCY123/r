journal = {Progress in Retinal and Eye Research},
keywords = {Anti-vascular endothelial growth factor,Asian age-related macular degeneration,Choroidal neovascularization,Laser photocoagulation,Photodynamic therapy,Polypoidal choroidal vasculopathy},
pages = {107--139},
