@article{Chakravarthy2013,
abstract = {Bevacizumab has been suggested to have similar effectiveness to ranibizumab for treatment of neovascular age-related macular degeneration. The Inhibition of VEGF in Age-related choroidal Neovascularisation (IVAN) trial was designed to compare these drugs and different regimens. Here, we report the findings at the prespecified 2-year timepoint. In a multicentre, 2×2 factorial, non-inferiority randomised trial, we enrolled adults aged at least 50 years with active, previously untreated neovascular age-related macular degeneration and a best corrected distance visual acuity (BCVA) of at least 25 letters from 23 hospitals in the UK. Participants were randomly assigned (1:1:1:1) to intravitreal injections of ranibizumab (0{\textperiodcentered}5 mg) or bevacizumab (1{\textperiodcentered}25 mg) in continuous (every month) or discontinuous (as needed) regimens, with monthly review. Study participants and clinical assessors were masked to drug allocation. Allocation to continuous or discontinuous treatment was masked up to 3 months, at which point investigators and participants were unmasked. The primary outcome was BCVA at 2 years, with a prespecified non-inferiority limit of 3{\textperiodcentered}5 letters. The primary safety outcome was arterial thrombotic event or hospital admission for heart failure. Analyses were by modified intention to treat. This trial is registered, number ISRCTN92166560. Between March 27, 2008, and Oct 15, 2010, 628 patients underwent randomisation. 18 were withdrawn; 610 received study drugs (314 ranibizumab; 296 bevacizumab) and were included in analyses. 525 participants reached the visit at 2 years: 134 ranibizumab in continuous regimen, 137 ranibizumab in discontinuous regimen, 127 bevacizumab in continuous regimen, and 127 bevacizumab in discontinuous regimen. For BCVA, bevacizumab was neither non-inferior nor inferior to ranibizumab (mean difference -1{\textperiodcentered}37 letters, 95% CI -3{\textperiodcentered}75 to 1{\textperiodcentered}01; p=0{\textperiodcentered}26). Discontinuous treatment was neither non-inferior nor inferior to continuous treatment (-1{\textperiodcentered}63 letters, -4{\textperiodcentered}01 to 0{\textperiodcentered}75; p=0{\textperiodcentered}18). Frequency of arterial thrombotic events or hospital admission for heart failure did not differ between groups given ranibizumab (20 [6%] of 314 participants) and bevacizumab (12 [4%] of 296; odds ratio [OR] 1{\textperiodcentered}69, 95% CI 0{\textperiodcentered}80-3{\textperiodcentered}57; p=0{\textperiodcentered}16), or those given continuous (12 [4%] of 308) and discontinuous treatment (20 [7%] of 302; 0{\textperiodcentered}56, 0{\textperiodcentered}27-1{\textperiodcentered}19; p=0{\textperiodcentered}13). Mortality was lower with continuous than discontinuous treatment (OR 0{\textperiodcentered}47, 95% CI 0{\textperiodcentered}22-1{\textperiodcentered}03; p=0{\textperiodcentered}05), but did not differ by drug group (0{\textperiodcentered}96, 0{\textperiodcentered}46-2{\textperiodcentered}02; p=0{\textperiodcentered}91). Ranibizumab and bevacizumab have similar efficacy. Reduction in the frequency of retreatment resulted in a small loss of efficacy irrespective of drug. Safety was worse when treatment was administered discontinuously. These findings highlight that the choice of anti-VEGF treatment strategy is less straightforward than previously thought. UK National Institute for Health Research Health Technology Assessment programme. Copyright {\textcopyright} 2013 Chakravarthy et al. Open Access article distributed under the terms of CC BY-NC-SA. Published by Elsevier Ltd. All rights reserved.},
author = {Chakravarthy, Usha and Harding, Simon P. and Rogers, Chris A. and Downes, Susan M. and Lotery, Andrew J. and Culliford, Lucy A. and Reeves, Barnaby C. and IVAN study investigators},
doi = {10.1016/S0140-6736(13)61501-9},
file = {:home/yincy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Chakravarthy et al. - 2013 - Alternative treatments to inhibit VEGF in age-related choroidal neovascularisation 2-year findings of the I.pdf:pdf},
issn = {1474547X},
journal = {Lancet},
number = {9900},
pages = {1258--1267},
pmid = {23870813},
publisher = {Chakravarthy et al. Open Access article distributed under the terms of CC BY-NC-SA},
title = {{Alternative treatments to inhibit VEGF in age-related choroidal neovascularisation: 2-year findings of the IVAN randomised controlled trial.}},
url = {http://dx.doi.org/10.1016/S0140-6736(13)61501-9},
volume = {382},
