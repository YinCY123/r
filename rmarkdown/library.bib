file = {:home/yincy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/L{\'{e}}veillard, Philp, Sennlaub - 2019 - Is retinal metabolic dysfunction at the center of the pathogenesis of age-related macular degenera.pdf:pdf},
issn = {14220067},
journal = {International Journal of Molecular Sciences},
keywords = {Aerobic glycolysis,Cone photoreceptor,Inflammatory macrophage,Lactate transporter,Retinal degeneration,Rod-derived cone viability factor},
number = {3},
title = {{Is retinal metabolic dysfunction at the center of the pathogenesis of age-related macular degeneration?}},
volume = {20},
year = {2019}
}
