file = {:home/yincy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sberro et al. - 2019 - Large-Scale Analyses of Human Microbiomes Reveal Thousands of Small, Novel Genes.pdf:pdf},
issn = {00928674},
journal = {Cell},
